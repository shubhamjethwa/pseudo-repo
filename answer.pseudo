#include<bits/stdc++.h>
using namespace std;

/**
  *		Logic for returning number in the range of 6 to 15.
  *		rand() % 10 will return in the range [0,9]
  *		After that we are adding 6 to it thus we'll have number in the range of [6,15].
  **/
void nameAndNumber(string name){
  int randomNumber = rand() % 10 + 6;
  cout<<name<<" "<<randomNumber<<"\n";
}

int main() {
  string name = "Shubham";
  nameAndNumber(name);
  return 0;
}
